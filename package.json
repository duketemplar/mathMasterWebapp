{
  "name": "app-test",
  "version": "0.0.1",
  "description": "app-test",
  "publishConfig": {
    "registry": "http://sinopia.prod.nordnet.se"
  },
  "config": {
    "dev_port": 9000
  },
  "scripts": {
    "env": "env",
    "e2e-test.install": "! [ -a 'tmp/selenium-server-standalone-2.45.0.jar' ] &&  wget -P tmp http://selenium-release.storage.googleapis.com/2.45/selenium-server-standalone-2.45.0.jar || echo 'selenium server already installed'",
    "start.ci": "webpack-dev-server --quiet --port $npm_package_config_dev_port --devtool eval --content-base src & echo $! > .start-ci.pid",
    "stop.ci": "[ -a '.start-ci.pid' ] && cat .start-ci.pid | xargs kill -9 || echo \"Already stopped\" && rm -f .start-ci.pid",
    "webpack": "webpack --colors --progress --config ./webpack.config.js",
    "build": "mkdir dist && NODE_ENV=production npm run webpack && cp src/index.html dist",
    "localbuild": "rm -rf dist && NODE_ENV=production npm run build && node localbuild.js",
    "e2e-test": "mkdir -p logs && nightwatch -t e2e-tests/hello-e2e-test.js -o reports -r junit",
    "e2e-test:jenkins": "npm run start.ci && sleep 60 && npm run e2e-test && npm run stop.ci",
    "prelint:jenkins": "rm -f reports/lintresult.xml && mkdir -p reports",
    "lint": "eslint --ext=.jsx --ext=.js -c .eslintrc src || echo \"Linting failed...\"",
    "lint:jenkins": "eslint -c .eslintrc src -f checkstyle  --ext=.jsx --ext=.js > reports/lintresult.xml",
    "test": "./node_modules/karma-cli/bin/karma start karma.conf",
    "test:debug": "./node_modules/karma-cli/bin/karma start karma.conf --no-single-run --reporters mocha,coverage --browsers Chrome",
    "test:tdd": "./node_modules/karma-cli/bin/karma start --auto-watch --no-single-run  karma.conf",
    "start": "node node_modules/webpack-dev-server/bin/webpack-dev-server.js  --devtool eval-source-map --host localhost --port 9000 --content-base src"
  },
  "files": [
    "dist"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "es5-shim": "*",
    "intl": "^0.1.4",
    "lodash": "^3.5.0",
    "native-promise-only": "^0.7.8-a",
    "normalize.css": "^3.0.3",
    "react": "^0.13.1",
    "react-intl": "^1.2.0",
    "react-router": "^0.13.1",
    "reflux": "^0.2.7"
  },
  "devDependencies": {
    "autoprefixer-core": "*",
    "autoprefixer-loader": "*",
    "babel": "^5.2.17",
    "babel-core": "*",
    "babel-eslint": "*",
    "babel-loader": "*",
    "babel-runtime": "*",
    "css-loader": "*",
    "es6-promise-polyfill": "*",
    "eslint": "^0.21.0",
    "eslint-plugin-react": "^2.2.0",
    "exports-loader": "^0.6.2",
    "file-loader": "*",
    "html-loader": "*",
    "imports-loader": "^0.6.3",
    "isparta-loader": "*",
    "jsx-loader": "*",
    "karma": "*",
    "karma-chai": "*",
    "karma-chai-plugins": "*",
    "karma-chrome-launcher": "*",
    "karma-cli": "*",
    "karma-common-js": "*",
    "karma-coverage": "*",
    "karma-junit-reporter": "*",
    "karma-mocha": "*",
    "karma-mocha-reporter": "*",
    "karma-phantomjs-launcher": "*",
    "karma-sourcemap-loader": "*",
    "karma-webpack": "*",
    "lodash": "*",
    "lolex": "^1.2.1",
    "mocha": "*",
    "nightwatch": "*",
    "postcss-loader": "*",
    "react-tools": "^0.13.3",
    "rewire": "*",
    "rewire-webpack": "*",
    "sass-loader": "0.4.2",
    "sinon": "*",
    "sinon-chai": "*",
    "style-loader": "*",
    "template-html-loader": "*",
    "webpack": "*",
    "webpack-dev-server": "^1.8.2"
  },
  "engines": {
    "node": ">=0.10.0"
  }
}